#include <stdlib.h>
#include <stdio.h>

typedef union documentos {
    int cpf;
    int RG;
    int CNH;

} Docs;

int main (void){

    Docs documento;

    printf("Digite o CPF: ");
    scanf("%d", &documento.cpf);

    printf("Digite o RG: ");
    scanf("%d", &documento.RG);

    printf("Digite o CNH: ");
    scanf("%d", &documento.CNH);

    printf(" \n Documentos: CPF %d RG%d CNH%d, documento.cpf, documento.RG, documento.CNH);


return 0; 
}

