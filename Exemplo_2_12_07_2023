#include <stdio.h>
#include <stdlib.h>

// Structs e ponteiros 

typedef struct disciplina {

    char nome [30];
    float nota;
}Disciplina


{
    /* data */
};


typedef struct aluno {

    char nome [20];
    int matricula;
    Disciplina * disciplina;


}Aluno;

int main (void){

Aluno * aluno = cadastra_aluno();

print("Dados: /n Nome: %s \n Matricula: %d \n Disciplina: %s \n, aluno->nome, aluno->matricula, aluno->disciplina->nome);


free(aluno->disciplina);
free(aluno);


// Aluno * aluno = (Aluno *) malloc (sizeof(Aluno));
        Aluno * aluno = (Aluno*) malloc(sizeof(Aluno));
        if (aluno==NULL){
            printf("Sem memoria \n");
            exit(1);
        }

        aluno->disciplina = (Disciplina*) malloc(sizeof(Disciplina));

printf("Digite o nome \n ");
scanf(" %[^\n], aluno->nome");
printf("Digite a natricula \n ");
scanf(" %d, &aluno->matricula");
printf ("Digite o nome da disciplina \n");
scanf(" %[^\n]", aluno->disciplina.nome)
printf("Dados: \n Nome: %s \n Matricula: %d \n Disciplina: %s \n, aluno->nome, aluno->matricula, aluno->disciplina.nome")


free(aluno)
